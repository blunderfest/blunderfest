{
  "workbench.editor.enablePreview": false,
  "editor.fontFamily": "'Monaspace Argon', monospace",
  "editor.fontLigatures": "'calt',  'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'liga'",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "explorer.compactFolders": false,
  "i18n-ally.localesPaths": ["assets/src/translations", "assets/lib/i18n"],
  "i18n-ally.enabledFrameworks": ["react-i18next"],
  "i18n-ally.namespace": true,
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.defaultNamespace": "translation"
}
